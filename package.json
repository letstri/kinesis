{
  "name": "@letstri/vue-kinesis",
  "version": "1.0.0",
  "description": "Easily create complexe interactive animations with Vue 3",
  "author": {
    "name": "Valerii Strilets",
    "email": "valerii.strilets@gmail.com"
  },
  "scripts": {
    "dev": "vite",
    "build": "vite build && vue-tsc --emitDeclarationOnly",
    "types": "vue-tsc",
    "preview": "vite preview"
  },
  "type": "module",
  "files": [
    "dist"
  ],
  "main": "./dist/vue-kinesis.umd.cjs",
  "module": "./dist/vue-kinesis.js",
  "exports": {
    ".": {
      "import": "./dist/vue-kinesis.js",
      "require": "./dist/vue-kinesis.umd.cjs"
    }
  },
  "types": "./dist/index.d.ts",
  "dependencies": {
    "vue": "^3.3.7"
  },
  "devDependencies": {
    "@types/node": "^20.8.9",
    "@vitejs/plugin-vue": "^4.4.0",
    "typescript": "^5.2.2",
    "vite": "^4.5.0",
    "vue-tsc": "^1.8.22"
  },
  "peerDependencies": {
    "vue": "^2.6.11"
  },
  "engines": {
    "node": ">=18"
  },
  "keywords": [
    "frontend",
    "animation",
    "interaction",
    "vue",
    "vue.js",
    "parallax",
    "mouse",
    "hover",
    "scroll",
    "gyroscope",
    "audio"
  ],
  "licence": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/letstri/vue-kinesis"
  }
